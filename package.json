{
  "name": "cms-image-lambda",
  "version": "0.0.0",
  "description": "Lambda image processing for cms.prx.org image uploads",
  "main": "index.js",
  "scripts": {
    "start": "npm test",
    "test": "mocha test/",
    "ci": "istanbul cover _mocha -- test/ && codecov",
    "watch": "mocha test/ -w",
    "build": "npm run build-sync && npm run build-prune && npm run build-zip",
    "build-sync": "rsync -crl --delete --exclude-from .lambdaignore . build",
    "build-prune": "cd build && npm prune --production > /dev/null",
    "build-zip": "rm -f build.zip && cd build && zip -rq ../build.zip ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PRX/cms-image-lambda"
  },
  "author": "ryan cavis <ryan@prx.org>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.4.7",
    "follow-redirects": "^1.2.3",
    "q": "^1.4.1",
    "sharp": "^0.17.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "dotenv": "^2.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "nock": "^9.0.9",
    "node-lambda": "^0.8.13",
    "sinon": "^1.17.5"
  }
}
